{
	"$schema": "https://json.schemastore.org/package",
	"ava": {
		"files": [
			"tests/**/*.ava.ts"
		],
		"typescript": {
			"compile": false,
			"rewritePaths": {
				"./": "dist/"
			}
		},
		"workerThreads": false
	},
	"dependencies": {
		"@google-labs/breadboard": "^0.5.0",
		"@google-labs/core-kit": "^0.0.1",
		"@google-labs/llm-starter": "^0.2.1",
		"jsonschema": "^1.4.1"
	},
	"devDependencies": {
		"@ava/typescript": "^4.1.0",
		"@types/node": "^20.9.0",
		"@typescript-eslint/eslint-plugin": "^6.10.0",
		"@typescript-eslint/parser": "^6.10.0",
		"ava": "^5.3.1",
		"eslint": "^8.53.0",
		"eslint-plugin-editorconfig": "^4.0.3",
		"eslint-plugin-promise": "^6.1.1",
		"rimraf": "^5.0.5",
		"ts-node": "^10.9.1",
		"tslib": "^2.6.2",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2"
	},
	"main": "./dist/index.js",
	"name": "@exadev/breadboard-kits",
	"packageManager": "yarn@4.0.1",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ExaDev-io/breadboard-kits.git"
	},
	"scripts": {
		"prebuild": "npm run clean",
		"build": "tsc --pretty",
		"build:publishable": "npm run build -- --project tsconfig.publish.json",
		"build:test": "npm run build -- --project tsconfig.json",
		"clean": "rimraf ./dist",
		"lint": "eslint ./src",
		"lint:fix": "npm run lint -- --fix",
		"once": "npx -y tsx ./src/index.ts",
		"prepack": "npm run build:publishable",
		"prepublishOnly": "npm run test && npm run build:publishable",
		"postpublish": "git push",
		"pretest": "npm run build:test",
		"start": "chmod +x ./src/index.ts && ./src/index.ts",
		"test": "npx -y madge --circular --warning src/ --extensions ts && FORCE_COLOR=1 ava"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"version": "0.3.17"
}
