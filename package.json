{
  "ava": {
    "files": [
      "tests/**/*.ts"
    ],
    "typescript": {
      "compile": false,
      "rewritePaths": {
        "./": "dist/"
      }
    },
    "workerThreads": false
  },
  "dependencies": {
    "@google-labs/breadboard": "^0.5.0",
    "@google-labs/core-kit": "^0.0.1",
    "@google-labs/llm-starter": "^0.2.1",
    "jsonschema": "^1.4.1"
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@types/node": "^20.9.0",
    "ava": "^5.3.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/src/index.js",
  "name": "@exadev/breadboard-kits",
  "packageManager": "yarn@4.0.1",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExaDev-io/breadboard-kits.git"
  },
  "scripts": {
    "build": "tsc --build --verbose",
    "clean": "tsc --build --clean",
    "once": "npx -y tsx ./src/index.ts",
    "prepublishOnly": "npm run test",
    "pretest": "npm run build",
    "start": "chmod +x ./src/index.ts && ./src/index.ts",
    "test": "FORCE_COLOR=1 ava"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.6"
}
